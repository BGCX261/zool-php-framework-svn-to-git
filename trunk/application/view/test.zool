<!-- InstanceBeginEditable name="content" --><h1>Simple Present</h1>

<!-- #BeginLibraryItem "/Library/300 x 250.lbi" -->

<div id="boxAd" style="float:right; margin-left: 10px; border-width: 1px 0px 1px 0px; border-color: #CCCCCC; border-style: dotted;">

<script type="text/javascript"><!--

